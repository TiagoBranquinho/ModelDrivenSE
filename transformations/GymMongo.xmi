<?xml version="1.0" encoding="ISO-8859-1"?>
<mdb:MongoDB xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:mdb="http://www.example.org/mongo" name="GymER">
  <collections name="Gym">
    <schema name="GymSchema">
      <fields name="city" isIndexed="true"/>
      <fields name="capacity" type="integer"/>
    </schema>
  </collections>
  <collections name="Client">
    <schema name="ClientSchema">
      <fields name="clientName"/>
      <fields name="clientDateOfBirth" type="date"/>
      <fields name="clientEmail" isIndexed="true"/>
      <fields name="clientPhoneNumber"/>
    </schema>
  </collections>
  <collections name="Staff">
    <schema name="StaffSchema">
      <fields name="staffName"/>
      <fields name="role"/>
      <fields name="staffDateOfBirth" type="date"/>
      <fields name="staffEmail" isIndexed="true"/>
      <fields name="staffPhoneNumber"/>
    </schema>
  </collections>
  <collections name="Plan">
    <schema name="PlanSchema">
      <fields name="tier"/>
      <fields name="price" type="float"/>
      <fields name="daysPerWeek" type="integer"/>
      <fields name="planID" type="integer" isIndexed="true"/>
    </schema>
  </collections>
  <collections name="Machine">
    <schema name="MachineSchema">
      <fields name="machineName"/>
      <fields name="model" isIndexed="true"/>
      <fields name="brand"/>
    </schema>
  </collections>
</mdb:MongoDB>
