<?xml version="1.0" encoding="UTF-8"?>
<gym:Gym xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gym="http://www.example.org/mongoGym"
    xsi:schemaLocation="http://www.example.org/mongoGym MongoGyms.ecore">
  <documents>
    <fields
        name="city"
        valueString="Porto"/>
    <fields
        name="capacity"
        valueInteger="200"/>
    <fields
        name="machines">
      <valueArray>
        <documentValues>
          <fields
              name="model"
              valueString="H01"/>
          <fields
              name="name"
              valueString="Chest Press"/>
          <fields
              name="brand"
              valueString="Hammer"/>
        </documentValues>
        <documentValues>
          <fields
              name="model"
              valueString="N01"/>
          <fields
              name="name"
              valueString="Hack Squat"/>
          <fields
              name="brand"
              valueString="Nautilus"/>
        </documentValues>
      </valueArray>
    </fields>
    <fields
        name="plan">
      <valueDocument>
        <fields
            name="tier"
            valueString="Standard"/>
        <fields
            name="price"
            valueInteger="30"/>
        <fields
            name="daysPerWeek"
            valueInteger="5"/>
        <fields
            name="clients">
          <valueArray>
            <values>Tiago</values>
            <values>Henrique</values>
            <values>Diogo</values>
            <values>Rui</values>
          </valueArray>
        </fields>
      </valueDocument>
    </fields>
  </documents>
</gym:Gym>
