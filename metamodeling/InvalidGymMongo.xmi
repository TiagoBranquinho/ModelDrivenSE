<?xml version="1.0" encoding="UTF-8"?>
<mdb:MongoDB
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:mdb="http://www.example.org/mongo"
    xsi:schemaLocation="http://www.example.org/mongo MongoDB.ecore"
    name="GymMongoInvalid">
  <collections
      name="Gym">
    <schema
        name="gymSchema">
      <fields
          name="city"
          isIndexed="true"/>
      <fields
          name="capacity"/>
      <fields
          name="plan"
          type="object">
        <objectSchema
            name="planSchema">
          <fields
              name="tier"/>
          <fields
              name="price"
              type="float"/>
          <fields
              name="daysPerWeek"
              type="integer"/>
          <fields
              name="planID"
              type="integer"/>
          <fields
              name="planID"/>
        </objectSchema>
      </fields>
      <fields
          name="machine"
          type="object">
        <objectSchema
            name="machineSchema">
          <fields
              name="machineName"/>
          <fields
              name="model"/>
          <fields
              name="brand"/>
        </objectSchema>
      </fields>
    </schema>
  </collections>
  <collections
      name="Gym">
    <schema
        name="gymSchemaDuplicate">
      <fields
          name="location"/>
    </schema>
  </collections>
  <collections
      name="Client">
    <schema
        name="clientSchema">
      <fields
          name="clientName"/>
      <fields
          name="clientDateOfBirth"
          type="date"/>
      <fields
          name="clientEmail"
          isIndexed="true"/>
      <fields
          name="clientPhoneNumber"/>
      <fields
          name="clientGym"
          type="objectID"/>
      <fields
          name="clientPlan"
          type="objectID"
          collectionReference="//@collections.3"/>
    </schema>
  </collections>
  <collections
      name="Staff">
    <schema
        name="staffSchema">
      <fields
          name="staffName"/>
      <fields
          name="role"/>
      <fields
          name="staffDateOfBirth"
          type="date"/>
      <fields
          name="staffEmail"
          isIndexed="true"/>
      <fields
          name="staffPhoneNumber"/>
      <fields
          name="staffGym"
          type="boolean"
          isIndexed="true"/>
    </schema>
  </collections>
  <collections
      name="Plan">
    <schema
        name="planSchema">
      <fields
          name="tier"/>
      <fields
          name="price"
          type="float"/>
      <fields
          name="daysPerWeek"
          type="integer"/>
      <fields
          name="planID"
          type="integer"/>
    </schema>
  </collections>
  <collections
      name="Machine">
    <schema
        name="machineSchema">
      <fields
          name="machineName"/>
      <fields
          name="model"/>
      <fields
          name="brand"/>
    </schema>
  </collections>
</mdb:MongoDB>
